package FullTest3;

import java.util.Scanner;

public class GameOfNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);

		int n1=s.nextInt();
		int n2=s.nextInt();
		if(n1>=n2) {
			int n=n1-n2;
			System.out.println(n);
			return;
		}
		int k=0;
	
		while(n1<=n2) {
			n1*=2;
			k++;
		}
		if(n1==n2) {
			System.out.print(k);
			
		}
		else {
			
			System.out.println(k+1);
		}

	}

}
